<?xml version="1.0"?>
<!--XMLCC 1.00 20150101 Amara Faith-->
<!--config file of the xmlcc library-->
<xmlcc:xmlcc>
  <!--library configuration; set by user-->
  <xmlcc:config>
    <!--parser configuration-->
    <xmlcc:parser>
      <xmlcc:console talk="no"/>
      <xmlcc:clean tags="yes" attributes="yes" comments="yes"/>
      <xmlcc:memory preallocation="999"/>
    </xmlcc:parser>
    <!--tokenizer configuration-->
    <xmlcc:tokenizer>
      <xmlcc:console talk="no"/>
    </xmlcc:tokenizer>
  </xmlcc:config>
  <!--library information; refreshed by each call-->
  <xmlcc:info>
    <xmlcc:built date="01.01.2015"/>
    <xmlcc:version number="1.00"/>
    <xmlcc:package name="Amara Faith"/>
    <xmlcc:license type="The MIT License (MIT)"/>
    <xmlcc:project url="https://github.com/graetz23/xmlcc/"/>
    <xmlcc:user name="Christian" email="graetz23@gmail.com"/>
  </xmlcc:info>
  <!--library system; run unit tests, etc.-->
  <xmlcc:system>
    <!--unit test framework configuration-->
    <xmlcc:test>
      <!--unit test framework configuration-->
      <xmlcc:sysList run="no"/>
      <xmlcc:sysStrTool run="no"/>
      <xmlcc:sysXmlTool run="no"/>
      <xmlcc:sysXmlParser run="no"/>
      <xmlcc:domTokenizer run="no"/>
      <xmlcc:domController run="no"/>
      <xmlcc:xmlcc run="no"/>
      <!--reads chars &compares them; due to use of STL file encoding-->
      <![CDATA[0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ]]>
    </xmlcc:test>
  </xmlcc:system>
</xmlcc:xmlcc>
